# Frontend Environment Variables
# Copy this to .env and replace with your actual values

# Google OAuth Client ID
# Get from: https://console.cloud.google.com/apis/credentials
# Authorized JavaScript origins: https://lovableproject.com, http://localhost:5173
VITE_GOOGLE_CLIENT_ID=107063868685-bsc76l303te03md39qk2i331nehc2ih1.apps.googleusercontent.com

# Razorpay Key ID (Publishable Key - safe for frontend)
# Get from: https://dashboard.razorpay.com/app/keys
VITE_RAZORPAY_KEY_ID=

# Backend API URL
# Development: http://localhost:3001
# Production: https://your-backend.com
VITE_API_BASE_URL=http://localhost:3001

# Mock Mode (set to 'false' when using real credentials)
# When true: uses localStorage and mock payment flows
# When false: connects to real backend and payment gateways
VITE_USE_MOCK=false
